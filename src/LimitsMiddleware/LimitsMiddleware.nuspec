<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>LimitsMiddleware</id>
        <version>0.0.0.0</version>
        <authors>Damian Hickey</authors>
        <owners>Damian Hickey</owners>
        <projectUrl>https://github.com/damianh/LimitsMiddleware</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <title>Limits Middleware</title>
        <description>
            Middleware that applies limits to an OWIN pipeline. Currently supported are max bandwidth, max concurrent requests, connection timeout,
            max request content length, max querystring length and max url length.
        </description>
        <tags>owin, middleware, throttling, limits</tags>
        <frameworkAssemblies />
        <releaseNotes>
            Remove dependency on owin.dll
        </releaseNotes>
    </metadata>
    <files>
        <file src="bin\Release\LimitsMiddleware.dll" target="lib\net45" />
        <file src="bin\Release\LimitsMiddleware.xml" target="lib\net45" />
    </files>
</package>